{
  "name": "my-react-kit",
  "version": "1.0.1",
  "description": "My simple SSR React-kit with hot reload and code splitting..",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev yarn run start:dev",
    "start:dev": "yarn run build:dev && webpack-dev-server --config=config",
    "start:prod": "yarn run build:prod && yarn run server",
    "build:dev": "yarn run clean && yarn run compile --progress --mode development",
    "build:prod": "yarn run clean && yarn run compile -p --progress --mode production",
    "compile": "webpack --config=config",
    "server": "babel-node ./src/app",
    "clean": "rm -rf ./dist/",
    "test": "yarn run eslint --fix ./src && yarn run jest"
  },
  "author": "Nasser Maronie",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "clean-webpack-plugin": "2.0.1",
    "copy-webpack-plugin": "5.0.2",
    "eslint": "5.16.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-react": "7.12.4",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.7.1",
    "react-hot-loader": "4.8.3",
    "redux-devtools-extension": "2.13.8",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "emotion": "10.0.9",
    "emotion-server": "10.0.9",
    "express": "4.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet-async": "1.0.2",
    "react-loadable": "5.5.0",
    "react-loadable-ssr-addon": "0.1.6",
    "react-redux": "7.0.2",
    "react-router": "5.0.0",
    "react-router-config": "5.0.0",
    "react-router-dom": "5.0.0",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.6.1",
    "smoothscroll": "0.4.0",
    "ua-parser-js": "0.7.19"
  },
  "jest": {
    "testURL": "http://localhost"
  }
}
