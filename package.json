{
  "name": "my-react-kit",
  "version": "1.0.0",
  "description": "My simple SSR React-kit with hot reload and code splitting..",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev yarn run start:dev",
    "start:dev": "yarn run build:dev && webpack-dev-server --config=config",
    "start:prod": "yarn run build:prod && babel-node src/server.js",
    "build:dev": "yarn run clean && yarn run compile:dev",
    "build:prod": "yarn run clean && yarn run compile:prod",
    "compile:dev": "webpack -p --progress --config=config --mode development",
    "compile:prod": "webpack -p --progress --config=config --mode production",
    "clean": "rm -rf ./dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nasser Maronie",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.8.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-loadable-ssr-addon": "^0.1.6"
  }
}
